/dts-v1/;

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

#include "qca953x.dtsi"

/ {
	compatible = "zbtlink,zbt-we826q", "qca,qca9531";
	model = "ZBT WE826-Q";

	aliases {
		label-mac-device = &wmac;
		led-failsafe = &led_power;
		led-running = &led_power;
		led-upgrade = &led_power;
	};

	chosen {
		bootargs = "console=ttyS0,115200";
		//stdout-path = "serial0:115200n8";
	};

	leds {
		compatible = "gpio-leds";

		led_power: power {
			label = "zbt-we826q:red:status";
			gpios = <&gpio 13 GPIO_ACTIVE_LOW>;
		};

		wlan2g {
			label = "zbt-we826q:blue:wlan2g";
			gpios = <&gpio 12 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "phy0tpt";
		};

		lan1 {
			label = "zbt-we826q:blue:lan1";
			gpios = <&gpio 16 GPIO_ACTIVE_LOW>;
		};

		lan2 {
			label = "zbt-we826q:blue:lan2";
			gpios = <&gpio 15 GPIO_ACTIVE_LOW>;
		};

		lan3 {
			label = "zbt-we826q:blue:lan3";
			gpios = <&gpio 14 GPIO_ACTIVE_LOW>;
		};

		lan4 {
			label = "zbt-we826q:blue:lan4";
			gpios = <&gpio 11 GPIO_ACTIVE_LOW>;
		};

		wan {
			label = "zbt-we826q:blue:wan";
			gpios = <&gpio 4 GPIO_ACTIVE_LOW>;
		};
	};

	keys {
		compatible = "gpio-keys";

		wps {
			label = "WPS button";
			linux,code = <KEY_WPS_BUTTON>;
			gpios = <&gpio 17 GPIO_ACTIVE_LOW>;
			debounce-interval = <60>;
		};
	};

	watchdog {
		compatible = "linux,wdt-gpio";

		gpios = <&gpio 2 GPIO_ACTIVE_HIGH>;
		hw_algo = "toggle";
		hw_margin_ms = <1200>;
		always-running;
	};
};

&spi {
	status = "okay";

	num-cs = <1>;

	flash@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <25000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			uboot: partition@0 {
				label = "u-boot";
				reg = <0x000000 0x020000>;
				read-only;
			};
			partition@20000 {
				compatible = "tplink,firmware";
				label = "firmware";
				reg = <0x020000 0xfd0000>;
			};

			art: partition@ff0000 {
				label = "art";
				reg = <0xff0000 0x010000>;
				read-only;
			};
		};
	};
};

&uart {
	status = "okay";
};

&usb0 {
	#address-cells = <1>;
	#size-cells = <0>;
	status = "okay";

	hub_port: port@1 {
		reg = <1>;
		#trigger-source-cells = <0>;
	};
};

&usb_phy {
	status = "okay";
};

&pcie0 {
	status = "okay";
};

&wmac {
	status = "okay";

	mtd-cal-data = <&art 0x1000>;
};

&mdio0 {
	status = "okay";
};

/* WAN */
&eth0 {
	status = "okay";

	phy-handle = <&swphy4>;

	mtd-mac-address = <&art 0x0>;
	mtd-mac-address-increment = <1>;

	fixed-link {
		speed = <100>;
		full-duplex;
	};
};

/* LAN */
&eth1 {
	status = "okay";

	mtd-mac-address = <&art 0x0>;
};